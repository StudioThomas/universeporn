{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 1,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 1,
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "prefer-destructuring": 1,
    "max-len": 1
  }
}
